<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.uIFragment">

    <View
        android:id="@+id/view3"
        android:layout_width="367dp"
        android:layout_height="120dp"
        android:background="#4FC8FF"
        app:layout_constraintBottom_toTopOf="@+id/view4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view2" />

    <View
        android:id="@+id/view2"
        android:layout_width="367dp"
        android:layout_height="120dp"
        android:background="#EC4747"
        app:layout_constraintBottom_toTopOf="@+id/view3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.522"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view" />

    <View
        android:id="@+id/view"
        android:layout_width="367dp"
        android:layout_height="120dp"
        android:layout_marginStart="9dp"
        android:layout_marginEnd="9dp"
        android:background="#6192DC"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.045" />

    <View
        android:id="@+id/view4"
        android:layout_width="367dp"
        android:layout_height="120dp"
        android:layout_marginBottom="148dp"
        android:background="#9777D1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.522"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view3" />

    <TextView
        android:id="@+id/Gross"
        android:layout_width="94dp"
        android:layout_height="73dp"
        android:layout_marginTop="40dp"
        android:text="12"
        android:textSize="48sp"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.93"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/Abertos"
        android:layout_width="0dp"
        android:layout_height="73dp"
        android:text="114"
        android:textSize="48sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/Pendentes"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/Pendentes"
        app:layout_constraintTop_toBottomOf="@+id/Cancelados"
        app:layout_constraintVertical_bias="0.158" />

    <TextView
        android:id="@+id/Pendentes"
        android:layout_width="0dp"
        android:layout_height="73dp"
        android:text="0"
        android:textSize="48sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/Gross"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/Gross"
        app:layout_constraintTop_toBottomOf="@+id/Abertos"
        app:layout_constraintVertical_bias="0.245" />

    <TextView
        android:id="@+id/Cancelados"
        android:layout_width="0dp"
        android:layout_height="73dp"
        android:text="40"
        android:textSize="48sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/Abertos"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/Abertos"
        app:layout_constraintTop_toBottomOf="@+id/Gross"
        app:layout_constraintVertical_bias="0.146" />

</androidx.constraintlayout.widget.ConstraintLayout>